<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Approved</title>
    <link rel="stylesheet" href="css/style.css">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">




    <script>
        function loadEvent() {
            console.log("kjbnd;kajbdkj;ab")
            fetch("https://long-teal-bandicoot-hem.cyclic.app/getApproved", {
                method: "GET",
                headers: {
                    "Content-Type": "application/json"
                },

            }).then((response) => response.json()).then((data) => {
                let placeholder = document.querySelector("#data-output");
                var originalData = JSON.parse(JSON.stringify(data));
                console.log(originalData.data);
                let out = "";
                for (let i of originalData.data) {
                    out += `
                    <tr><td>${i.id}</td>
                        <td>${i.eventName}</td>
                        <td>${i.clubName}</td>
                        <td>${i.venue}</td>
                        <td>${i.eventDate}</td>
                        <td>${i.cordinatorName}</td>
                        <td>${i.cordinatorNo}</td>
                        <td>${i.expectedAudience}</td>      
    
                   `;
                }
                placeholder.innerHTML = out;
            });
        }
    </script>
</head>

<body onload="loadEvent()">
    <div class="sidebar">
        <!-- The sidebar -->
        <div class="sidebar">
            <div class="img">
                <img src="images/EveLogLogo.jpg">
            </div>
            <a href="/home">Home</a>
            <a href="/eventProposal ">EventProposal</a>
            <a href="/eventRequested ">eventsRequested</a>
            <a class="active " href="/eventApproved ">eventsApproved</a>
            <a href="/eventRejected ">eventsRejected</a>
        </div>
    </div>
    <div class="content ">

        <div class="navHeader ">
            <nav class="navbar navbar-expand-lg bg-body-tertiary ">

                <div class="container-fluid ">
                    <a class="navbar-brand " href="# ">
                        <h4>Events Approved</h4>
                    </a>
                    <button class="navbar-toggler " type="button " data-bs-toggle="collapse " data-bs-target="#navbarSupportedContent " aria-controls="navbarSupportedContent " aria-expanded="false " aria-label="Toggle navigation ">
                      <span class="navbar-toggler-icon "></span>
                  </button>
                    <div class="collapse navbar-collapse " id="navbarSupportedContent ">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0 "> </ul>
                        <form class="d-flex " role="search ">
                            <input class="form-control me-2 " type="search " placeholder="Search " aria-label="Search ">
                            <button class="btn btn-outline-success " type="submit ">Search</button>
                        </form>
                    </div>
                </div>
            </nav>
        </div>



        <div class="container ">
            <div class="row ">
                <div class="col-12 ">
                    <table class="table table-bordered ">
                        <thead>
                            <tr>
                                <th scope="col ">eventId</th>
                                <th scope="col ">EventName</th>
                                <th scope="col ">ClubName</th>
                                <th scope="col ">Venue</th>
                                <th scope="col ">Dates</th>
                                <th scope="col ">CordinatorName</th>
                                <th scope="col ">CordinatorNo</th>
                                <th scope="col ">expectedAudience</th>
                                <!-- <th scope="col ">Actions
                       
                        </th> -->
                            </tr>
                        </thead>
                        <tbody id="data-output">

                        </tbody>

                    </table>
                </div>
            </div>
        </div>
    </div>
</body>

</html>